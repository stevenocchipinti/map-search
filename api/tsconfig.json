{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "skipLibCheck": true,
    "noEmit": false,
    "outDir": ".vercel/output"
  },
  "include": ["**/*.ts", "../src/lib/**/*.ts", "../src/types/**/*.ts"]
}
